<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>aoa.ac</path>

  <animation>
    <object-name>AOA.fond</object-name>
    <object-name>AOA.buttons</object-name>
    <object-name>AOA.face</object-name>
    <object-name>AOA.needle</object-name>

    <object-name>AOA.vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>AOA.vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>AOA.vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>AOA.face</object-name>
    <object-name>AOA.needle</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>AOA.needle</object-name>
    <property>orientation/alpha-deg</property>
    <interpolation>
      <entry><ind>  0 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 14 </ind><dep> 226.5 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>


<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>asi.ac</path>
  
  <animation>
    <object-name>ASI.fond</object-name>
    <object-name>ASI.face</object-name>
    <object-name>ASI.needle</object-name>
    <object-name>ASI.needle1</object-name>
    <object-name>index.pointer1</object-name>
    <object-name>index.pointer2</object-name>
    
    <object-name>ASI.vitre</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Fairchild-Metroliner/Models/Effects/Glass/glass</inherits-from>
    <object-name>ASI.vitre</object-name>
  </effect>
  
  <animation>
    <type>material</type>
    <object-name>ASI.face</object-name>
    <object-name>ASI.needle</object-name>
    <object-name>ASI.needle1</object-name>
    <object-name>index.pointer1</object-name>
    <object-name>index.pointer2</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>ASI.needle1</object-name>
    <property>instrumentation/airspeed-indicator/limit-indicated-speed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep> 178 </dep></entry>
      <entry><ind> 170 </ind><dep> 178 </dep></entry>
      <entry><ind> 240 </ind><dep> 268 </dep></entry>
      <entry><ind> 300 </ind><dep> 333 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>ASI.needle</object-name>
    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  30 </ind><dep>   0 </dep></entry>
      <entry><ind>  40 </ind><dep>  10 </dep></entry>
      <entry><ind> 100 </ind><dep>  88 </dep></entry>
      <entry><ind> 170 </ind><dep> 178 </dep></entry>
      <entry><ind> 240 </ind><dep> 268 </dep></entry>
      <entry><ind> 300 </ind><dep> 333 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>index.pointer1</object-name>
    <property>instrumentation/airspeed-indicator/index[0]</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  30 </ind><dep>   0 </dep></entry>
      <entry><ind>  40 </ind><dep>  10 </dep></entry>
      <entry><ind> 100 </ind><dep>  88 </dep></entry>
      <entry><ind> 170 </ind><dep> 178 </dep></entry>
      <entry><ind> 240 </ind><dep> 268 </dep></entry>
      <entry><ind> 300 </ind><dep> 333 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>index.pointer2</object-name>
    <property>instrumentation/airspeed-indicator/index[1]</property>
    <interpolation>
      <entry><ind>   0 </ind><dep>   0 </dep></entry>
      <entry><ind>  30 </ind><dep>   0 </dep></entry>
      <entry><ind>  40 </ind><dep>  10 </dep></entry>
      <entry><ind> 100 </ind><dep>  88 </dep></entry>
      <entry><ind> 170 </ind><dep> 178 </dep></entry>
      <entry><ind> 240 </ind><dep> 268 </dep></entry>
      <entry><ind> 300 </ind><dep> 333 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
